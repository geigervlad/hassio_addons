{
  "name": "OnlyOffice",
  "url": "https://github.com/geigervlad/hassio_addons/tree/master/nextcloud",
  "version": "7.2",
  "slug": "onlyoffice",
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "description": "OnlyOffice for Nextcloud on Hassio",
  "startup": "system",
  "boot": "manual",
  "map": [
    "share:rw",
    "media:rw",
    "ssl:rw"
  ],
  "ports": {
    "443/tcp": 8444,
    "80/tcp": 8082
  },
  "ports_description": {
    "443/tcp": "https port",
    "80/tcp": "http port"
  },
  "webui": "https://[HOST]:[PORT:443]",
  "panel_icon": "mdi:cloud",
  "backup_exclude": [],
  "privileged": [
    "SYS_ADMIN"
  ],
  "init": false,
  "image": "onlyoffice/documentserver"
}